{
	"name": "Time Capsule",
	"version": "1.0",
	"description": "Build an Extension!",
	"permissions": ["storage","http://*/", "https://*/"],
	"background": {
		"scripts": ["background.js","popup.js"],
		"persistent": false
	},
	"manifest_version": 2,
	// "page_action": {
	//       "default_popup": "popup.html"
	// },
	"icons": {
		"128": "icon_128.png"
	},
	"browser_action": {
		"default_title": "Time Capsule",
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
